{
    "name": "ts-seed-web",
    "version": "1.0.1",
    "main": "dist/index.js",
    "license": "MIT",
    "type": "module",
    "dependencies": {
        "@koa/router": "^12.0.1",
        "dotenv": "^16.4.5",
        "koa": "^2.15.3",
        "source-map-support": "^0.5.21",
        "vite-node": "^2.0.5"
    },
    "scripts": {
        "debug": "vite-node --inspect=0.0.0.0:9229 src/index.ts",
        "debug:watch": "vite-node --inspect=0.0.0.0:9229 --watch src/index.ts",
        "build": "tsc && tsc-alias",
        "lint": "biome format && biome lint",
        "lint:fix": "npm run lint -- --fix",
        "lint:ci": "biome ci",
        "prepare": "husky",
        "test": "vitest",
        "test:ci": "vitest --coverage.enabled=true --reporter=basic --coverage.reporter=text --coverage.reporter=json",
        "deps:ci": "depcruise --output-type err src",
        "deps:graph": "depcruise --output-type dot src | dot -T svg > dependency-graph.svg",
        "deps:report": "depcruise --output-type err-html --output-to dependency-report.html src"
    },
    "devDependencies": {
        "@biomejs/biome": "1.8.3",
        "@types/koa": "2.15.0",
        "@types/koa__router": "12.0.4",
        "@types/node": "20.14.14",
        "@vitest/coverage-v8": "2.0.5",
        "biome": "0.3.3",
        "dependency-cruiser": "16.3.10",
        "husky": "9.1.4",
        "lint-staged": "15.2.8",
        "prettier": "3.3.3",
        "tsc-alias": "1.8.10",
        "typescript": "5.5.4",
        "vite-tsconfig-paths": "4.3.2",
        "vitest": "2.0.5"
    },
    "lint-staged": {
        "*.{ts,tsx}": ["npm run lint:fix", "depcruise --validate -- "]
    },
    "overrides": {}
}
